generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//Modeling

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model TruckCategory {
  id   Int    @id @default(autoincrement())
  name String
  weight Int
  imageUrl String
  capacity Int
  length Float
  width  Float
  height Float
}